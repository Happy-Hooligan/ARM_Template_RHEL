{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "vmName": {
        "value": "anseeble"
      },
      "authenticationType": {
        "value": "password"
      },  
      "adminUsername": {
        "value": "cysadmin"
      },
      "rhelOSVersion": {
        "value": "82gen2",
        "metadata": {
            "description": "Get-AzVMImageSku -Location westus2 -PublisherName RedHat -Offer RHEL  Get-AzVMImageOffer -Location westus2 -PublisherName center-for-internet-security-inc"
        }
      },
      "vmSize": {
        "value": "Standard_B2ms",
        "metadata": {
            "description": "Get-AzVMSize -Location westus2 | where { $_.numberofcores -eq 2 }"
        }
      },
      "virtualNetworkName": {
        "value": "Site-to-site-VPN",
        "metadata": {
            "description": "Get-AzVirtualNetwork | select name,subnets"
        }
      },
      "subnetName": {
        "value": "VPN-Gateway",
        "metadata": {
            "description": "(get-azVirtualNetwork -Name Automation_Resources-vnet).Subnets.name"
        }
      },
      "networkSecurityGroupName": {
        "value": "iamAnNSG"
      },
      "dnsLabelPrefix": {
        "value": "dnslabelprefix"
      }
    }
  }